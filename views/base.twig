  {% block head%}  
    {% include 'head.twig' %}
    </head>
  {% endblock %}
  <body class="{{body_class}}">
    <a class="skip-link screen-reader-text" href="#content">{{ 'Skip to content' }}</a>
    <header>
      {% block header %}
        {% include 'header.twig' %}
      {% endblock %}
    </header>
    <main id="content" role="main" class="content-wrapper">
      {% block content %}
        Content
      {% endblock %}
    </main>
    <footer>
    {% block footer %}
      {% include 'footer.twig' %}
    {% endblock %}
    </footer>
    {{ function('wp_footer') }}
  </body>
</html>